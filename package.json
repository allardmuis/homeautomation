{
  "name": "homeautomation",
  "version": "0.1.0",
  "description": "System to automate home central heating and oters",
  "engines": {
    "node": "10.x"
  },
  "main": "build/server.js",
  "scripts": {
    "start": "node build/server.js",
    "postinstall": "npm run build-ts",
    "build-ts": "tsc --build tsconfig.json",
    "watch-node": "nodemon build/server.js",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "express": "~4.16.4",
    "@ksyos/koi": "~2.1.0"
  },
  "devDependencies": {
    "@types/express": "~4.16.0",
    "@types/node": "~10.12.5",
    "typescript": "~3.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/allardmuis/homeautomation.git"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
